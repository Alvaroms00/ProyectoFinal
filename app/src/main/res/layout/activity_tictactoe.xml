<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:onClick="ponerFicha"
    tools:context=".Juegos.TresEnRaya.TictactoeActivity">

    <ImageView
        android:id="@+id/celda_1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:contentDescription="@string/celda"
        android:scaleType="fitXY"
        android:src="@drawable/circulo"
        app:layout_constraintBottom_toTopOf="@id/celda_5"
        app:layout_constraintDimensionRatio="1"
        app:layout_constraintEnd_toStartOf="@id/celda_5"
        app:layout_constraintWidth_percent="0.25" />

    <ImageView
        android:id="@+id/celda_2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:contentDescription="@string/celda"
        android:scaleType="fitXY"
        android:src="@drawable/circulo"
        app:layout_constraintBottom_toTopOf="@+id/celda_5"
        app:layout_constraintDimensionRatio="1"
        app:layout_constraintStart_toStartOf="@+id/celda_5"
        app:layout_constraintWidth_percent="0.25" />

    <ImageView
        android:id="@+id/celda_3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:contentDescription="@string/celda"
        android:scaleType="fitXY"
        android:src="@drawable/circulo"
        app:layout_constraintBottom_toTopOf="@id/celda_5"
        app:layout_constraintDimensionRatio="1"
        app:layout_constraintStart_toEndOf="@+id/celda_5"
        app:layout_constraintWidth_percent="0.25" />

    <ImageView
        android:id="@+id/celda_4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:contentDescription="@string/celda"
        android:scaleType="fitXY"
        android:src="@drawable/circulo"
        app:layout_constraintBottom_toBottomOf="@+id/celda_5"
        app:layout_constraintDimensionRatio="1"
        app:layout_constraintEnd_toStartOf="@+id/celda_5"
        app:layout_constraintWidth_percent="0.25" />

    <ImageView
        android:id="@+id/celda_5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:contentDescription="@string/celda"
        android:scaleType="fitXY"
        android:src="@drawable/circulo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintDimensionRatio="1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent="0.25" />

    <ImageView
        android:id="@+id/celda_6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:contentDescription="@string/celda"
        android:scaleType="fitXY"
        android:src="@drawable/circulo"
        app:layout_constraintBottom_toBottomOf="@+id/celda_5"
        app:layout_constraintDimensionRatio="1"
        app:layout_constraintStart_toEndOf="@+id/celda_5"
        app:layout_constraintWidth_percent="0.25" />

    <ImageView
        android:id="@+id/celda_7"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:contentDescription="@string/celda"
        android:scaleType="fitXY"
        android:src="@drawable/circulo"
        app:layout_constraintDimensionRatio="1"
        app:layout_constraintEnd_toStartOf="@+id/celda_5"
        app:layout_constraintTop_toBottomOf="@+id/celda_5"
        app:layout_constraintWidth_percent="0.25" />

    <ImageView
        android:id="@+id/celda_8"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:contentDescription="@string/celda"
        android:scaleType="fitXY"
        android:src="@drawable/circulo"
        app:layout_constraintDimensionRatio="1"
        app:layout_constraintStart_toStartOf="@+id/celda_5"
        app:layout_constraintTop_toBottomOf="@+id/celda_5"
        app:layout_constraintWidth_percent="0.25" />

    <ImageView
        android:id="@+id/celda_9"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:contentDescription="@string/celda"
        android:scaleType="fitXY"
        android:src="@drawable/circulo"
        app:layout_constraintDimensionRatio="1"
        app:layout_constraintStart_toEndOf="@id/celda_5"
        app:layout_constraintTop_toBottomOf="@id/celda_5"
        app:layout_constraintWidth_percent="0.25" />

    <!--Lineas divisorias-->
    <FrameLayout
        android:layout_width="0dp"
        android:layout_height="4dp"
        android:background="@color/black"
        app:layout_constraintBottom_toTopOf="@id/celda_5"
        app:layout_constraintEnd_toEndOf="@id/celda_3"
        app:layout_constraintStart_toStartOf="@id/celda_1" />

    <FrameLayout
        android:layout_width="0dp"
        android:layout_height="4dp"
        android:background="@color/black"
        app:layout_constraintEnd_toEndOf="@id/celda_3"
        app:layout_constraintStart_toStartOf="@id/celda_1"
        app:layout_constraintTop_toBottomOf="@id/celda_5" />

    <FrameLayout
        android:layout_width="4dp"
        android:layout_height="0dp"
        android:background="@color/black"
        app:layout_constraintBottom_toBottomOf="@id/celda_9"
        app:layout_constraintStart_toStartOf="@id/celda_5"
        app:layout_constraintTop_toTopOf="@id/celda_3" />

    <FrameLayout
        android:layout_width="4dp"
        android:layout_height="0dp"
        android:background="@color/black"
        app:layout_constraintBottom_toBottomOf="@id/celda_9"
        app:layout_constraintStart_toEndOf="@id/celda_5"
        app:layout_constraintTop_toTopOf="@id/celda_3"/>
</androidx.constraintlayout.widget.ConstraintLayout>